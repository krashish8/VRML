#VRML V2.0 utf8
#Traffic Light system - Ground Component
#Designed by Colm O'Carra

#This wrl object is the ground for the main world and is also made using 
#Inlined Objects.
Group {
   children [
      #ground
      Shape {
	appearance Appearance {
	    	texture ImageTexture {
                   #To give a texture to the green area
                   url "floor.jpg"
                }
           
	}
	geometry Box {
	    size 100.0 0.1 100.0
	}
      },
      #The road along the z-axis
      Shape {
	appearance Appearance {
	    material Material { 
	    	diffuseColor 0.272 0.272 0.272
            }
	}
	geometry Box {
	    size 15.0 0.12 100.0
	}
      },
      #The traffic island on that road
      Transform { 
         translation -0.2 0.0 8.8
            children [
               Inline {
                 url "TrafficIsland.wrl"
               }
            ]
      },
      Transform {
         translation 0.2 0.0 -8.8
         rotation 0.0 1.0 0.0 3.14
            children [
               Inline {
                 url  "TrafficIsland.wrl"
               }
            ]
      }, 
