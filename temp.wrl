#VRML V2.0 utf8

#Set Background
Background {
	skyAngle [   2 ]
	skyColor [    0.6 1 1, 1 0.8 0.6 ]
	groundAngle [ 0.5, 1, 1.53 ]
	groundColor [ 0 0 0, 0.2 0.5 0, 0.3 0.4 0, 0.8 0.6 0.4 ]
}

#Set ViewPoint
Viewpoint { 
   position  0 0 0
}

NavigationInfo {
  avatarSize [0.25, 3.3, 0.75]
  speed 20.0
  type "WALK"
}

#Increase frame size
Shape {
    geometry Box {
    size 20000 10000 20000
  }
  appearance Appearance {
           material Material { 
      diffuseColor 1 1 1
      transparency 0
     }
    }
}



#Primary Ground
Transform{
   translation 0 -5.1 0
   children[
      Shape {
         geometry Box {
           size 20000 10 20000
         }
         appearance Appearance {
            texture ImageTexture {
               url "images/ground.jpg"
               repeatS FALSE
               repeatT FALSE
            }
         }
      }
   ]
}


DEF MerryGoRound Transform {
  translation -30 1 -90
  scale 1 1 1
  children [
   Inline {
    url "merry-go-round.wrl"
    }
  ]
}

DEF Slide Transform {
translation -50 2 -50
scale 1 2 1
children [
 Inline {
  url "slide.wrl"
  }
]
}


DEF Tree Transform {
translation 50 0 -50
scale 1 1 1
children [
 Inline {
  url "tree1.wrl"
  }
]
}


DEF Plane Transform {
translation 3500 1500 -5000
scale 15 15 15
children [
 Inline {
  url "random-models/plane.wrl"
  }
]
}


DEF timer TimeSensor { 
      cycleInterval 20
      loop TRUE 
}

DEF pi PositionInterpolator { 
               key [ 0 10 ] 
               keyValue [3500 1500 -5000, -102500 1500 -5000] 
} 


ROUTE timer.fraction_changed TO pi.set_fraction 
ROUTE pi.value_changed TO Plane.set_translation 


DEF Spiral Transform {
translation 0 0 -50
scale 1 1 1
children [
 Inline {
  url "spiral.wrl"
  }
]
}



EXTERNPROTO TheSwingSet [
 field SFVec3f translation
 exposedField SFRotation rotation
 exposedField SFVec3f scale
]
"swing/theswingset.wrl.gz#TheSwingSet"

EXTERNPROTO TheWell [
 field SFVec3f translation
 exposedField SFRotation rotation
 exposedField SFVec3f scale
]
"swing/thewell.wrl.gz#TheWell"


DEF ThePicnicPlace Group {
  children [
    TheSwingSet {
      translation 20 -5 -50
  rotation 0 1 0 0
  scale 0.1 0.1 0.1
    }
    TheWell {
      translation 20 0 0
  rotation 0 1 0 0
  scale 2.5 2.5 2.5
    }
]
}


