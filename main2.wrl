#VRML V2.0 utf8

#Set Background
Background {
	skyAngle [   2 ]
	skyColor [    0.6 1 1, 1 0.8 0.6 ]
	groundAngle [ 0.5, 1, 1.53 ]
	groundColor [ 0 0 0, 0.2 0.5 0, 0.3 0.4 0, 0.8 0.6 0.4 ]
}

#Set ViewPoint
Viewpoint { 
   position  0 0 100
}

NavigationInfo {
  avatarSize [0.25, 3.3, 0.75]
  speed 20.0
  type "WALK"
}

DEF TheSun DirectionalLight {
  direction 0 -1 0
  ambientIntensity 1
}

#Increase frame size
Shape {
    geometry Box {
    size 20000 10000 20000
  }
  appearance Appearance {
           material Material { 
      diffuseColor 1 1 1
      transparency 0
     }
    }
}

#Secondary Ground
Transform{
   translation 0 -5.2 0
   children[
      Shape {
         geometry Box {
           size 20000 10 20000
         }
         appearance Appearance {
            texture ImageTexture {
               url "swing/thesurface.jpg"
               repeatS FALSE
               repeatT FALSE
            }
         }
      }
   ]
}


#Primary Ground
Transform{
   translation 0 -5.1 0
   children[
      Shape {
         geometry Box {
           size 210 10 210
         }
         appearance Appearance {
            texture ImageTexture {
               url "swing/thesurface.jpg"
               repeatS FALSE
               repeatT FALSE
            }
         }
      }
   ]
}


