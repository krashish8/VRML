EXTERNPROTO TheWell [
 field SFVec3f translation
 exposedField SFRotation rotation
 exposedField SFVec3f scale
]
"swing/thewell.wrl.gz#TheWell"


DEF ThePicnicPlace Group {
  children [
    TheSwingSet {
      translation 60 -5 -10
  rotation 0 1 0 1.57
  scale 0.1 0.1 0.1
    }
    TheTeeterTotter {
      translation 40 3 35
  rotation 0 1 0 0
  scale 0.2 0.2 0.2
    }
    TheTeeterTotter {
      translation 60 3 35
  rotation 0 1 0 3.14
  scale 0.2 0.2 0.2
    }
    TheWell {
      translation 20 0 0
  rotation 0 1 0 0
  scale 2.5 2.5 2.5
    }
]
}

Transform{
  
