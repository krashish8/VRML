EXTERNPROTO TheSwingSet [
 field SFVec3f translation
 exposedField SFRotation rotation
 exposedField SFVec3f scale
]
"playground/PicnicGroup/theswingset.wrl.gz#TheSwingSet"

EXTERNPROTO TheTeeterTotter [
 field SFVec3f translation
 exposedField SFRotation rotation
 exposedField SFVec3f scale
]
"playground/PicnicGroup/theteetertotter.wrl.gz#TheTeeterTotter"

EXTERNPROTO TheWell [
 field SFVec3f translation
 exposedField SFRotation rotation
 exposedField SFVec3f scale
]
"playground/PicnicGroup/thewell.wrl.gz#TheWell"


DEF ThePicnicPlace Group {
  children [
    TheSwingSet {
      translation 60 -5 -10
  rotation 0 1 0 1.57
  scale 0.1 0.1 0.1
    }
    TheTeeterTotter {
      translation 40 3 35
  rotation 0 1 0 0
  scale 0.2 0.2 0.2
    }
    TheTeeterTotter {
      translation 60 3 35
  rotation 0 1 0 3.14
  scale 0.2 0.2 0.2
    }
    TheWell {
      translation 20 -0.1 0
  rotation 0 1 0 0
  scale 2.5 2.5 2.5
    }
    Transform{
      translation 20 0.9 0
      children[
        Shape {
           geometry Box {
             size 3.2 1.9 3.2
             solid FALSE
           }
           appearance Appearance {
              texture ImageTexture {
                 url "playground/well.jpg"
                 repeatS FALSE
                 repeatT FALSE
              }
           }
        }
      ]
    }
    Transform{
      translation 20 0.9 0
      children[
        Shape {
           geometry Box {
             size 3 2 3
             solid FALSE
           }
           appearance Appearance {
              material Material { 
                diffuseColor 0.5 0.5 0.5

               }
           }
        }
      ]
    }
    Transform {
    translation 20 6.6 0
    scale 0.5 0.5 0.5
    children [
     Inline {
      url "playground/bartwist.wrl"
      }
    ]
    }
]
}

Transform{
  
