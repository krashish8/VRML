#VRML V2.0 utf8


PROTO Fleche [
	    field SFVec3f size 0.5 0.5 0.05
	    field SFColor color 1 1 1
	    field SFVec3f translation 1.2 3 0
	   ]
{
   Transform {
      translation IS translation
      rotation 0 0 1 0.7853981
children [
	 Shape {
	    appearance Appearance {
	       material Material {
		  emissiveColor IS color
	       }
	    }
	    geometry Box {
	       size IS size
	    }
	 }
      ]
   }}
PROTO Panneau [
	    field SFVec3f size 1 0.5 0.1
	    field SFColor color 1 1 1
	    field SFVec3f translation 0.7 3 0
	   ]
{
   Transform {
      translation IS translation
children [
	 Shape {
	    appearance Appearance {
 texture ImageTexture {
		      url "sortie.jpg"
 }}	     
	    geometry Box {
	       size IS size
	    }
	 }
      ]
   }}

Fleche {
}
Panneau {
