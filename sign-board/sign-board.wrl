#VRML V2.0 utf8

PROTO Tube [
field SFFloat radius 0.2
field SFFloat lenght 5
field SFColor color 0 1 0
field SFVec3f translation 0 2.5 0
]
{
Transform {
translation IS translation
children [
   Shape {
      appearance Appearance {
         material Material {
diffuseColor IS color
}}
      geometry Cylinder {
height IS lenght
radius IS radius
}}
 ]
}} 
PROTO Fleche [
	    field SFVec3f size 1 1 0.05
	    field SFColor color 1 1 1
	    field SFVec3f translation -1 1.5 0
	   ]
{
   Transform {
      translation IS translation
      rotation 0 0 1 0.7853981
children [
	 Shape {
	    appearance Appearance {
	       material Material {
		  emissiveColor IS color
	       }
	    }
	    geometry Box {
	       size IS size
	    }
	 }
      ]
   }}

PROTO Panneau [
	    field SFVec3f size 1 1.2 0.1
	    field SFColor color 1 1 1
	    field SFVec3f translation -0.5 1.5 0
	   ]
{
   Transform {
      translation IS translation
children [
	 Shape {
	    appearance Appearance {
 texture ImageTexture {
		      url "jeu.jpg"
 }}	     
	    geometry Box {
	       size IS size
	    }
	 }
      ]
   }}
