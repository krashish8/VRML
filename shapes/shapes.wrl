#vent
Group {
   children [
      Transform {
	 translation 0 0.48 0.2
	 children [
	    Inline {
	       url "rotate_button.wrl"
	    }
	   ]
      },
DEF VENT TouchSensor {},
]
}

DEF TIME TimeSensor {cycleInterval 5}
DEF ORIENTATION OrientationInterpolator {
key [0 0.5 1]
keyValue [0 1 0  0, 0 1 0  3.14, 0 1 0 6.28] 
}
ROUTE VENT.touchTime TO TIME.startTime
ROUTE TIME.fraction_changed TO ORIENTATION.set_fraction
ROUTE ORIENTATION.value_changed TO ALOUETTE.rotation
