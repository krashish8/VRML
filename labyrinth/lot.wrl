      level	[
		 # LEVEL III: The world
		 Inline {
		    url "entry3.wrl"
		 }

		 # LEVEL II: The world from quite near
		 # Inline {
		 #   url "entry2.wrl"
		 # }

		 # LEVEL I: Some very simple geometry
