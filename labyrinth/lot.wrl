	DEF FrontView-A-5 Viewpoint {
	  position 0 1 30
        orientation 0 1 0 0
	  fieldOfView 1.3
	  description "E5: Front View"
	}

	DEF SkyView-A-5 Viewpoint {
	  position 0 100 0
	  orientation 1.0 0.0 0.0 -1.571
	  description "E5: Sky View"
	}
       ]
     } # End Viewpoints


Background {


skyAngle     [ 0.7, 1.2, 1.6, 2.2 ]

skyColor     [ 0 0 0.9, 0 0.2 0.7, 0 0.3 0.6, 0 0.4 0.3]
 }

     #
     # A floor
     #
     Inline {
       url "floor.wrl"
     }

     #
